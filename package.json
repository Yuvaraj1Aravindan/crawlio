{
  "name": "crawler-gcp-k8s",
  "version": "1.0.0",
  "description": "A powerful web crawling and scraping service - Crawler Edition",
  "main": "src/server.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "nodemon src/server.js",
    "dev:frontend": "cd frontend && npm run dev -- -p 3001",
    "start": "node src/server.js",
    "build": "cd frontend && npm run build",
    "test": "jest",
    "docker:build": "docker-compose build",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down"
  },
  "keywords": ["crawling", "scraping", "api", "web-scraping", "firecrawl"],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "dotenv": "^16.3.1",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "playwright": "^1.39.0",
    "cheerio": "^1.0.0-rc.12",
    "redis": "^4.6.10",
    "pg": "^8.11.3",
    "stripe": "^14.7.0",
    "joi": "^17.11.0",
    "multer": "^1.4.5-lts.1",
    "aws-sdk": "^2.1490.0",
    "bull": "^4.12.0",
    "winston": "^3.11.0",
    "compression": "^1.7.4",
    "express-rate-limit": "^7.1.5",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "concurrently": "^8.2.2"
  },
  "engines": {
    "node": ">=18.17.0"
  }
}
